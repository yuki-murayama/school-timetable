# 開発環境設定ファイル
# フロントエンド開発サーバー
VITE_FRONTEND_PORT=5174

# バックエンドAPI URL（固定ポート）
VITE_API_URL=http://localhost:8787/api

# 開発環境識別
NODE_ENV=development

# 開発用認証設定
VITE_AUTH_PROVIDER=custom

# 認証設定（削除済み - 自前JWT認証を使用）
# VITE_CLERK_PUBLISHABLE_KEY=（削除済み）

# その他の開発用設定
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug